.field__title--radius {
  padding-bottom: 20rpx;
}

.userinfo image {
  width: 750rpx;
  height: 338rpx;
}

.popup-field-example--bottom {
  width: 100%;
  height: 300rpx;
}

.picker-view-example {
  height: 240rpx;
}

.picker-view-column-example {
  line-height: 100rpx;
  text-align: center;
}

.form_input {
  width: 100%;
  height: 100rpx;
  border-bottom: solid 1px #ff6500;
  border-top: none;
  padding-top: 20rpx;
}

.form_subbtn {
  padding-top: 40rpx;
  width: 100%;
  height: 100rpx;
}

.subbtn {
  width: 600rpx;
  height: 92rpx;
  border-radius: 46rpx;
  background-color: #ff6500;
  font-size: 36rpx;
  font-weight: Regular;
  font-family: "PingFang-SC-Regular";
}

.form_input label {
  float: left;
  font-size: 36rpx;
  line-height: 80rpx;
  height: 80rpx;
  width: 160rpx;
  text-align: center;
  font-family: "PingFang-SC-Bold";
  font-weight: Bold;
}

.form_input input {
  padding-left: 10rpx;
  line-height: 80rpx;
  height: 80rpx;
  width: 400rpx;
  float: left;
}

.form_input button {
  width: 200rpx;
  float: right;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 60rpx;
  border: solid 2rpx #ff6500;
  color: #ff6500;
  font-weight: 300;
}

.nav_subbtn {
  padding-top: 40rpx;
  width: 100%;
  margin: 0 auto;
  height:50rpx
}

.nav_subbtn navigator {
  width: 30%;
  text-align: center;
  color: #888;
  float: left;
  font-family: "PingFang-SC-Medium";
  font-weight: Medium;
  font-size: 28rpx;
  margin-top:10rpx;
}

.nav_subbtn text {
  float: right;
  width: 30%;
  text-align: center;
  color: #888;
  font-family: "PingFang-SC-Medium";
  font-weight: Medium;
  font-size: 28rpx;
}

.nav_subbtn button {
  float: right;
  width: 30%;
  text-align: center;
  color: #888;
  font-family: "PingFang-SC-Medium";
  font-weight: Medium;
  font-size: 28rpx;
  height: 40rpx;
  line-height: 40rpx;
  background-color: white;
  margin-top:10rpx;
}

button::after{ border: none; } 

.nav_subbtn view {
  float: left;
  width: 5%;
  margin: 0 auto;
  text-align: center;
}

