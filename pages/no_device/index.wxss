.form_input{
  width: 80%;
  height: 120rpx;
  border-bottom: solid 1px #FF6500;
  border-top: none;
  padding-top: 40rpx;
}

.form_subbtn{
  padding-top: 40rpx;
  width: 80%;
  height: 100rpx;
}

.subbtn{
   width: 600rpx; 
