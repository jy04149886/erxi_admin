}

.form_input label{
  float: left;
  font-size: 48rpx;
  line-height: 80rpx;
  height: 80rpx;
  font-weight: 900;
  width:160rpx;
  text-align: center;
}

.form_input input{
  padding-left:10rpx;
  line-height: 80rpx;
  height: 80rpx;
  width: 300rpx;
  float:left;

}

.form_input button{
  width: 200rpx;
  float: right;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 60rpx;
  border: solid 2rpx #FF6500;
  color: #FF6500;
  font-weight: 300;
}

